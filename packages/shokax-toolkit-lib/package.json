{
  "name": "shokax-toolkit-lib",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "bun@1.3.0",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./posts/*": {
      "types": "./src/posts/*.ts",
      "import": "./src/posts/*.ts"
    }
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "check-types": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:e2e": "echo 'No test specified' && exit 0",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix"
  },
  "dependencies": {
    "@antfu/eslint-config": "^6.6.1",
    "@ctrl/tinycolor": "^4.2.0",
    "eslint": "^9.39.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.15"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vite": "^7.2.7"
  }
}
